    "toolkit",
    "css library"
  ],
  "author": "estrattonbailey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/estrattonbailey/svbstrate/issues"
  },
  "homepage": "http://estrattonbailey.com/svbstrate-site/",
  "dependencies": {
    "fs-extra": "^0.30.0"
  },
  "devDependencies": {
    "cssnano": "^3.10.0",
    "postcss": "^6.0.2",
    "postcss-cli": "^4.1.0",
    "postcss-import": "^10.0.0"
  }
}
