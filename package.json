{
  "name": "svbstrate",
  "version": "4.1.2",
  "description": "A simple, scalable CSS base layer.",
  "main": "dist/svbstrate.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/estrattonbailey/svbstrate.git"
  },
  "scripts": {
    "start": "NODE_ENV=development postcss src/main.css -w -o dist/svbstrate.css",
